pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
-- picodachi 0.1.2
-- tiny fun frends for life
cartname = 'picodachi'

-- be prepared
save(cartname .. '_backup')

-- start off with small pico
-- feed pico
-- defend pico
-- give toys to pico
-- plants
-- map

-- pico grows sprite size ++
-- build house for pico
-- picobux
-- shoppe

function get(addr)
	local val = 0x0
	val = bor(val, peek(addr+1))
	val = bor(shl(val, 8), peek(addr))
	return val
end

function set(addr, val)
	poke(addr, val)
	poke(addr+1, shr(val, 8))
end

-- note: cannot name this 'save'
function sav(addr, val)
	if(val) set(addr, val)
	cstore(addr, addr, 2)
end

-- convention: _ after name
-- refers to an address
init_ = 0xf00 -- semi-intentional
px_ = 0xf02
py_ = 0xf04
flip_ = 0xf06
grow_ = 0xf08
cx_ = 0xf0a
cy_ = 0xf0c
sky_ = 0xf0e
gtime_ = 0xf10

function initvars()
	if(get(init_) == 1) return
	sav(init_, 1)
	sav(px_, 20)
	sav(py_, 104)
	sav(flip_, 1)
	sav(grow_, 1)
	sav(cx_, 0)
	sav(cy_, 0)
	sav(sky_, 1)
	sav(gtime_, 0)
end

ticks = 0

function _update()
	-- since time resets with cart
	-- gtime tracks in-game time
	sav(gtime_, get(gtime_)+1)
	local gtime = get(gtime_)
	
	if(btnp(4, 0)) then
	 set(init_, 0)
		initvars()
	end

	local px = get(px_)
	local cx = get(cx_)
	local flip = get(flip_)
	local sky = get(sky_)
	
	if(btnp(5, 0)) then
		sav(sky_, get(sky_)%4+1)
	end
	
	-- increment sky colors
	if(gtime%132 == 128) then
		sky = (sky+1)%4
	end
	if(gtime%132 == 0) then
		sky = (sky+1)%4
	end
	
	-- update stats
	--  hunger
	--  tiredness
	--  health
	--  

	-- decide what to do
	
	-- if food, eat
	-- if enemy, run
	-- if bored, sleep
	-- else move
	
	if(px > 350 or px < 4) then
		flip *= -1
	end
	px += flip
	
	-- move camera to follow
	if(px-cx > 100 and cx < 384) then
		cx += 1
	end
	if(px-cx < 12 and cx > 0) then
		cx -= 1
	end
	

	-- done with variables
	sav(px_, px)
	sav(cx_, cx)
	sav(flip_, flip)
	sav(sky_, sky)
	
	-- save to rom once per second
	ticks += 1
	if(ticks == 30) then
		save(cartname)
		ticks = 0
	end
end

growseq = {0,1,2,4}
skyseq = {12,13,1,13}

function _draw()
	local sky = get(sky_)
	local gtime = get(gtime_)

	cls()

	-- draw sky and sun/moon
	rectfill(0,0,128,128,skyseq[sky])
	spr(39+flr(gtime/132)%2, 110, gtime%132-12)
	
	camera(get(cx_), get(cy_))

	-- background layer
	mapdraw(0,0,0,0,64,16)
	
	local grow = get(grow_)
	
	-- picoda sprite
	if(grow < 3) then
		spr(growseq[grow],
			get(px_), get(py_), 1, 2,
			get(flip_)==1)
	elseif(grow < 5) then
		spr(growseq[grow],
			get(px_), get(py_), 2, 2,
			get(flip_)==1)
	end
	
	-- foreground layer
	mapdraw(0,16,0,0,64,16)


-- gui
	camera()
	
	print(stat(0), 0, 0)
	print(stat(1), 0, 8)
end

function _init()
	initvars(false)
	music(0)
end
