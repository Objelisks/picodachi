pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
-- picodachi 0.1
-- tiny fun frends for life
cartname = 'picodachi'

-- be prepared
save(cartname .. '_backup')

-- start off with small pico
-- feed pico
-- defend pico
-- give toys to pico
-- plants
-- map

-- pico grows sprite size ++
-- build house for pico
-- picobux
-- shoppe

function get(addr)
	local val = 0x0
	val = bor(val, peek(addr+1))
	val = bor(shl(val, 8), peek(addr))
	return val
end

function set(addr, val)
	poke(addr, val)
	poke(addr+1, shr(val, 8))
end

-- note: cannot name this 'save'
function sav(addr, val)
	if(val) set(addr, val)
	cstore(addr, addr, 2)
end

-- convention: _ after name
-- refers to an address
init_ = 0xf00
px_ = 0xf02
py_ = 0xf04
flip_ = 0xf06
grow_ = 0xf08

function initvars()
	if(get(init_) == 1) return
	sav(init_, 1)
	sav(px_, 20)
	sav(py_, 104)
	sav(flip_, 1)
	sav(grow_, 1)
end

ticks = 0

function _update()
	if(btnp(4, 0)) then
	 set(init_, 0)
		initvars()
	end
	if(btnp(5, 0)) then
		sav(grow_, get(grow_)+1)
	end
	
	local px = get(px_)
	local flip = get(flip_)

	if(px > 110 or px < 4) then
		flip *= -1
	end
	px += flip
	
	ticks += 1
	if(ticks == 30) then
		save(cartname)
		ticks = 0
	end

	sav(px_, px)
	sav(flip_, flip)
end

growseq = {0,1,2,4}


function _draw()
	cls()

	-- background layer
	mapdraw(0,0,0,0,16,16)
	
	local grow = get(grow_)
	
	print(grow, 8, 8, 4)
	print(growseq[grow], 16, 8, 4)
	
	
	-- picoda sprite
	if(grow < 3) then
		spr(growseq[grow],
			get(px_), get(py_), 1, 2,
			get(flip_)==1)
	elseif(grow < 5) then
		spr(growseq[grow],
			get(px_), get(py_), 2, 2,
			get(flip_)==1)
	end
	
	-- foreground layer
	mapdraw(0,16,0,0,16,16)
	
end

function _init()
	initvars(false)
	music(0)
end






